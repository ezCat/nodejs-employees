<%- include('navbar-header'); %>


<table class="users bordered striped">
<thead>
  <tr>
    <th>Moyenne</th>
    <th>Maximum</th>
    <th>Minimum</th>
  </tr>
</thead>

<tbody>
<% for(var i=0; i<stats.length; i++) {%>
  <tr class="stat">
    <td><%= stats[i].min %></td>
    <td><%= stats[i].max %></td>
    <td><%= stats[i].avg %></td>
  </tr>
<% } %>

</tbody>


</table>

